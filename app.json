{
  "expo": {
    "name": "TooDoo",
    "description": "TooDoo is a task management application.",
    "slug": "TooDoo",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "fi.tamk.toodoo"
    },
    "android": {
      "package": "fi.tamk.toodoo"
    }
  }
}
